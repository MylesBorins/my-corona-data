{
  "name": "my-corona-data",
  "version": "1.0.0",
  "description": "Just looking into data",
  "main": "index.js",
  "scripts": {
    "start": "node --experimental-modules index.mjs",
    "update-data": "npm run update-subtree && npm run fetch-state-data",
    "fetch-state-data": "node ./scripts/update-state-data.mjs",
    "update-subtree": "git subtree pull --prefix third_party/coronavirus-data https://github.com/nychealth/coronavirus-data master --squash -m \"update city data\"",
    "test": "npm run lint",
    "lint": "eslint *.mjs lib/*.mjs",
    "fix-lint": "npm run lint -- --fix"
  },
  "author": "Myles Borins <mylesborins@google.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "csv-parse": "^4.8.8",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0"
  }
}
